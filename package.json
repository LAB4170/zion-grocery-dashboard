{
  "name": "zion-grocery-dashboard",
  "version": "1.0.0",
  "description": "Point of Sale System for Zion Groceries",
  "main": "backend/server.js",
  "scripts": {
    "start": "cd backend && node server.js",
    "build": "cd backend && npm install --production",
    "dev": "cd backend && npm run dev",
    "migrate": "cd backend && npm run migrate",
    "setup": "cd backend && npm install && cd ../frontend && npm install && cd ../backend && npm run migrate",
    "render-build": "cd backend && npm install --production && cd ../frontend && npm install",
    "render-start": "cd backend && node server.js",
    "install-all": "cd backend && npm install && cd ../frontend && npm install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LAB4170/zion-grocery-dashboard.git"
  },
  "keywords": [
    "pos",
    "grocery",
    "dashboard",
    "nodejs",
    "postgresql"
  ],
  "author": "Zion Groceries",
  "license": "MIT",
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "dependencies": {
    "axios": "^1.6.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "express": "^4.18.2",
    "express-rate-limit": "^6.7.0",
    "helmet": "^7.0.0",
    "knex": "^2.4.2",
    "morgan": "^1.10.0",
    "pg": "^8.16.3",
    "redis": "^4.6.0",
    "socket.io": "^4.8.1",
    "uuid": "^10.0.0"
  }
}
